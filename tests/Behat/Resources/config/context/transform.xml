<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <defaults public="true" />

        <service id="phpmud.behat.context.transform.location" class="PHPMud\Tests\Behat\Context\Transform\LocationContext">
            <argument type="service" id="phpmud.repository.location" />
        </service>

        <service id="phpmud.behat.context.transform.direction" class="PHPMud\Tests\Behat\Context\Transform\DirectionContext" />
    </services>
</container>
